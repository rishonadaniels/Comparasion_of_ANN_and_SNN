   "source": [
    "# Define model, loss function, and optimizer\n",
    "device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
    "model = FeedforwardMLP().to(device)\n",
    "print(next(model.parameters()).device)\n"
   ]
  },
  {
