   "source": [
    "# Hyperparameters\n",
    "#input_size = 784  # 28x28 images flattened\n",
    "#hidden_size = 128\n",
    "#num_classes = 10\n",
    "num_epochs = 200\n",
    "batch_size = 512\n",
    "learning_rate = 0.001"
   ]
  },
  {
